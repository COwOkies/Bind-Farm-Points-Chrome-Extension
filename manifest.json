{
  "manifest_version": 2,
  "name": "Bing Points Auto",
  "version": "1.0",
  "description": "Cette extension vous ouvre vos pages bing pour farmer les points de recherche pc.",
  
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://www.bing.com/"
  ]
}
